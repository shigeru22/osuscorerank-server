{
	"name": "osuinactive-api",
	"version": "0.1.0",
	"description": "API for osu! Inactive User Score Rankings",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon --exec npx ts-node src/index.ts",
		"fetch": "ts-node fetch/index.ts",
		"build": "rimraf ./dist && tsc && cd client && npm run build-wasm && npm install && npm run build && move-cli ./build ../dist/client && cd .. && cp-cli ./package.json ./dist/package.json && rimraf ./deta/dist && move-cli ./dist ./deta/dist && ts-node ./src/utils/post-build/index.ts",
		"clean": "rimraf ./dist",
		"start": "node .",
		"deploy": "cd ./deta && deta deploy"
	},
	"author": "shigeru22",
	"license": "MIT",
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/inquirer": "^8.2.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/lodash": "^4.14.178",
		"@types/node": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"cp-cli": "^2.0.0",
		"eslint": "^8.6.0",
		"eslint-plugin-import": "^2.25.4",
		"inquirer": "^8.2.0",
		"move-cli": "^2.0.0",
		"nodemon": "^2.0.15",
		"rimraf": "^3.0.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"axios": "^0.25.0",
		"cors": "^2.8.5",
		"deta": "^1.1.0",
		"dotenv": "^11.0.0",
		"express": "^4.17.3",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21"
	}
}
